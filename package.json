{
  "name": "fun-tour",
  "private": true,
  "version": "1.0.0",
  "description": "Fun Tour frontend",
  "main": "index.js",
  "repository": "github.com:asfamilybank/fun-tour.git",
  "author": "tanggangchao <tanggangchao@outlook.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest --coverage",
